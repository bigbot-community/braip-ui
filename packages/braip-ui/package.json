{
  "name": "@bigbot-community/bigbot-ui",
  "version": "1.0.1",
  "description": "Biblioteca de componentes Vue3 com styleguide e documentação para o design system BigBot.",
  "type": "module",
  "main": "./dist/braip-ui.cjs",
  "module": "./dist/braip-ui.js",
  "types": "./dist/types/index.d.ts",
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "import": "./dist/braip-ui.js",
      "require": "./dist/braip-ui.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./styles": "./dist/style.css",
    "./styles/*": "./dist/styles/*",
    "./components/*": {
      "import": "./dist/components/*/index.js",
      "types": "./dist/types/components/*/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && vue-tsc --emitDeclarationOnly --declaration --outDir dist/types",
    "test": "pnpm exec vitest",
    "test:coverage": "pnpm exec vitest run --coverage",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint src --ext .ts,.vue",
    "lint:fix": "eslint . --ext .ts,.vue --fix"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.1",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^17.3.0",
    "jiti": "^2.6.1",
    "jsdom": "^26.1.0",
    "sass": "^1.70.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.56.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^1.2.0",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bigbot-community/braip-ui.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "components",
    "ui",
    "braip",
    "design-system"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigbot-community/braip-ui/issues"
  },
  "homepage": "https://braip-ui.dev"
}
